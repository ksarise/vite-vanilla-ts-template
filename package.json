{
  "name": "vite-vanilla-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "npx eslint src/**/*.ts",
    "format": "npx prettier --write .",
